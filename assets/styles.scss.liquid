a.image-link{
  width: 100%;
  position:relative;
  display:inline-block;
  &:before{
    content: "";
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    background-color: #000;
    opacity: 0;
    filter: alpha(Opacity=0);
    visibility: hidden;
    transition: all .2s;
  }
  &:hover{
    &:before{
      opacity: .1;
      filter: alpha(Opacity=10);
      visibility: visible;
    }
  }
}
