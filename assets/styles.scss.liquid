                margin-top: -25px;
                left: 50%;
                right: auto;
                margin-left: -48px;
                position: absolute;
                opacity: 0;
                visibility: hidden;
                transition: .1s all;
                line-height: 1.4;
                z-index: 9;
                background: #000;
                color: #fff;
              i{opacity: 0;transition: all 0.3s;&:before {opacity: 0;}}
              span {margin-left:-17px;
              transition: all 0.3s;}
              &.loading {
                i{opacity: 1;}
                span {margin-left:0;}
              }
            }
            .actions {
              bottom: -56px;
              text-align: right;
              padding: 0;
              opacity: 1;
              visibility: visible;
              width: auto;
              right: 0;
              left: auto;
              position: absolute;
              transition: opacity .2s;
              .action-list{
              	&.addtocart {
                  display: none;
                }
                .add-to-links{
                	li a.link-wishlist {
                      background-color: transparent;
                      border: 0;
                      color: #dfdfdf;
                      font-size: 17px;
                      display: inline-block;
                      height: 32px;
                      width: 32px;
                      line-height: 32px;
                      text-align: center;
                  }
                }
              }
            }
            .bottom-home-dailydeal{
            	bottom: 0;
            }
            .product-image{
              &::after{
              	content: "";
                background: #000;
                position: absolute;
                left: 0;
                top: 0;
                width: 100%;
                height: 100%;
                opacity: 0;
                z-index: 2;
                visibility: hidden;
                transition: all 0.3s;
              }
            }
          }
          .details-area {
            padding: 10px 0;
            padding-top: 30px;
            text-align: left;
            .product-name {
              padding-right: 30px;
              letter-spacing: 0.025em;
            }
            .price {
              color: #000;
              font-family: "Oswald";
              font-weight: 400;
              margin: 10px 0 5px;
              font-size: 18px;
            }
            .actions {
              display: none;
            }
            .ratings{
            	background: none;
            }
          }
        }
        &:hover{
          .product-image-area{
            .product-image{
              &::after{
              	opacity: 0.2;
    			visibility: visible;
              }
            }
            .quickview-icon {
                opacity: 1;
                visibility: visible;
            }
            .actions{
              .action-list{
                .add-to-links{
                  li a.link-wishlist {
                    color: #000;
                  }
                }
              }
            }
          }
        }
      }
      .item.product-layout-list{
        .item-area{
          .details-area{
            padding: 0px 20px;
            .actions {
              display: block;
              .action-list{
                i.icon-cart{
                  line-height: 27px;
                }
              }
            }
          }
          .product-image-area{
            .actions{
            	display: none;
            }
            .quickview-icon{
            	display: none;
            }
            .product-image::after{
            	content: none;
            }
          }
        }
      }
      &.move-action{
        .item{
          .item-area{
            .product-image-area{
              .actions{
                display:block;
                bottom: 10px;
                left: 10px;
                right: auto;
                z-index:10;
                a.quickview{
                  display:none;
                }
              }
            }
          }
        }
      }
      &.hide-action{
        .item{
          .item-area{
            .product-image-area{
              .actions{
                  display:none;
              }
            }
          }
        }
      }
    }
  }
}
