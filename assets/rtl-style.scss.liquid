  border-radius: 6px;
  -webkit-box-shadow: 0 0 5px rgba(0,0,0,0.5);
  box-shadow: 0 0 5px rgba(0,0,0,0.5);
  &.animate-box {
    overflow: visible;
    height: auto;
    display:none;
    transition:.4s;
