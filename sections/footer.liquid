      <div class="row">
        <div class="col-sm-12">
          <div class="footer-top-inner">
            <div class="row">
              <div class="col-md-5">
                <div class="block" style="margin-bottom: 15px;">
                  <div class="block-title footer-block-title">
                    <h3>
                      <span>
                        {% if settings.language_enable %}
