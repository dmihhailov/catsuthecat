        <span class="lang2">{{ section.settings.footer_top_custom_1 | split: '[lang2]' | last }}</span>
        {% else %}
        {{ section.settings.footer_top_custom_1 | split: '[lang2]' | first }}
        {% endif %}  
      </div>
      <div class="col-md-6 a-right-sm">
        <div style="float:left;">
          {% if settings.language_enable %}
          <span class="lang1">{{ section.settings.footer_top_custom_2 | split: '[lang2]' | first }}</span>
          <span class="lang2">{{ section.settings.footer_top_custom_2 | split: '[lang2]' | last }}</span>
          {% else %}
          {{ section.settings.footer_top_custom_2 | split: '[lang2]' | first }}
          {% endif %}           
        </div>
        <div>
          {% include 'footer_top_subscrible' %}
        </div>
      </div>
      {% endif %}
